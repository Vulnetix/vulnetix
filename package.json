{
  "name": "vulnetix",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "generate-types": "wrangler types && prisma generate",
    "build": "npm run generate-types && vite build --clearScreen --mode development",
    "watch": "npm run generate-types && vite build --watch --clearScreen --mode development --sourcemap inline",
    "preview": "wrangler pages dev ./dist",
    "lint": "npm run generate-types && eslint . -c .eslintrc.js --fix --ext .ts,.js,.vue,.tsx --ignore-path .gitignore",
    "build:icons": "node src/@iconify/build-icons.js",
    "postinstall": "npm run build:icons",
    "deploy": "npm run generate-types && vite build --clearScreen --mode production && wrangler pages deployment create ./dist --project-name vulnetix --branch staging --upload-source-maps=true"
  },
  "dependencies": {
    "@casl/vue": "^2.2.0",
    "@floating-ui/dom": "^1.2.8",
    "@mdi/font": "^7.4.47",
    "@pandatix/js-cvss": "^0.4.4",
    "@prisma/adapter-d1": "^6.5.0",
    "@prisma/client": "^6.9.0",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/core": "^10.1.2",
    "@vueuse/math": "^10.1.2",
    "@wdns/vue-code-block": "^2.3.3",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "anylogger-console": "^1.0.0",
    "apexcharts": "^4.7.0",
    "axios": "^1.3.1",
    "chart.js": "^4.1.2",
    "cookie": "^1.0.2",
    "crypto-es": "^2.1.0",
    "disposable-email": "^0.2.3",
    "dompurify": "^3.2.1",
    "email-providers": "^2.7.0",
    "jose": "^5.9.6",
    "jwt-decode": "^3.1.2",
    "marked": "^15.0.2",
    "pinia": "^2.1.3",
    "roboto-fontface": "^0.10.0",
    "sass": "^1.59.3",
    "semver": "^7.6.3",
    "ssvc": "^0.1.1",
    "ulog": "2.0.0-beta.19",
    "unplugin-fonts": "^1.3.1",
    "vue": "^3.3.4",
    "vue-chartjs": "^5.2.0",
    "vue-google-charts": "^1.1.0",
    "vue-router": "^4.2.1",
    "vue3-apexcharts": "^1.4.1",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.8.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@cyclonedx/yarn-plugin-cyclonedx": "^1.0.2",
    "@iconify-json/bx": "^1.1.6",
    "@iconify-json/bxl": "^1.1.6",
    "@iconify-json/bxs": "^1.1.6",
    "@iconify-json/mdi": "^1.1.52",
    "@iconify/tools": "^4.1.2",
    "@iconify/utils": "^2.1.5",
    "@iconify/vue": "^4.1.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "boxicons": "^2.1.4",
    "prisma": "^6.9.0",
    "typescript": "^5.8.3",
    "unplugin-auto-import": "^0.16.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.5.6",
    "vite-plugin-pages": "^0.30.1",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-vuetify": "^2.1.1",
    "wrangler": "^4.4.0"
  },
  "packageManager": "yarn@4.9.2"
}
